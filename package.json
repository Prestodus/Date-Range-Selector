{
    "name":  "date-range-selector-card",
    "version":  "0.0.9",
    "description":  "A Home Assistant Lovelace card for selecting date ranges",
    "type":  "module",
    "main":  "dist/date-range-selector-card.js",
    "scripts":  {
                    "build":  "rollup -c",
                    "watch":  "rollup -c -w",
                    "lint":  "eslint src --ext .ts",
                    "format":  "prettier --write \"src/**/*.ts\"",
                    "prepare":  "husky install",
                    "release:local":  "npm ci \u0026\u0026 npm run build \u0026\u0026 git add dist \u0026\u0026 git commit -m \"chore(release): build dist for v${npm_package_version}\" || echo \u0027No changes to commit\u0027"
                },
    "keywords":  [
                     "home-assistant",
                     "lovelace",
                     "custom-card",
                     "date-range"
                 ],
    "author":  "Prestodus",
    "license":  "MIT",
    "dependencies":  {
                         "date-fns":  "^2.30.0",
                         "lit":  "^2.8.0"
                     },
    "devDependencies":  {
                            "@rollup/plugin-commonjs":  "^25.0.7",
                            "@rollup/plugin-json":  "^6.1.0",
                            "@rollup/plugin-node-resolve":  "^15.2.3",
                            "@rollup/plugin-terser":  "^0.4.4",
                            "@rollup/plugin-typescript":  "^11.1.5",
                            "@types/node":  "^20.10.5",
                            "@typescript-eslint/eslint-plugin":  "^6.15.0",
                            "@typescript-eslint/parser":  "^6.15.0",
                            "eslint":  "^8.56.0",
                            "husky":  "^8.0.3",
                            "lint-staged":  "^14.0.1",
                            "prettier":  "^3.1.1",
                            "rollup":  "^4.9.0",
                            "tslib":  "^2.6.2",
                            "typescript":  "^5.3.3"
                        },
    "husky":  {
                  "hooks":  {

                            }
              },
    "lint-staged":  {
                        "src/**/*.{ts,js}":  [
                                                 "npm run lint",
                                                 "npm run format",
                                                 "git add"
                                             ]
                    }
}